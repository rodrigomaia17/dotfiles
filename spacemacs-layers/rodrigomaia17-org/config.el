(setq
 ;; OrgMode
 org-log-done nil
 org-agenda-files (list "~/notas/")
 ;; set up org mode
 org-startup-indented t
 org-startup-folded "showall"
 org-directory "~/notas"
 org-enable-org-journal-support t
 org-journal-dir "~/notas/journal/"
 org-journal-date-format "%A, %B %d %Y"
 org-journal-time-prefix "* ")
